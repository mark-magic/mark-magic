{
  "name": "@mami/cli",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "bin": {
    "mami": "./bin.js"
  },
  "exports": "./dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --sourcemap --shims && tsup src/bin.ts --format esm --target esnext",
    "dev": "tsup-node src/index.ts --format esm --dts --sourcemap --shims && tsup src/bin.ts --format esm --target esnext --watch",
    "setup": "pnpm build",
    "start": "tsx ./src/bin.ts",
    "test": "vitest run"
  },
  "sideEffects": false,
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.8.3",
    "tsup": "^6.2.3",
    "tsx": "^3.10.1",
    "typescript": "^4.8.4",
    "vitest": "^0.24.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@liuli-util/async": "^3.6.0",
    "@liuli-util/fs-extra": "^0.1.0",
    "bundle-require": "^3.1.0",
    "esbuild": "^0.15.10",
    "form-data": "^4.0.0",
    "lodash-es": "^4.17.21",
    "node-fetch": "^3.2.10",
    "ora": "^6.1.2",
    "type-fest": "^3.1.0"
  }
}
