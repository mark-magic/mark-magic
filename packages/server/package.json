{
  "name": "@mami/server",
  "private": true,
  "version": "0.2.0",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": "./*"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "setup": "pnpm build",
    "test": "vitest run",
    "start": "tsx watch src/bin.ts"
  },
  "sideEffects": false,
  "devDependencies": {
    "@types/configstore": "^6.0.0",
    "@types/koa": "^2.13.5",
    "@types/koa-logger": "^3.1.2",
    "@types/koa-static": "^4.0.2",
    "@types/koa__cors": "^3.3.0",
    "@types/koa__router": "^12.0.0",
    "tsup": "^6.2.3",
    "tsx": "^3.9.0",
    "typescript": "^4.8.4",
    "vitest": "^0.23.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.0",
    "@mami/cli": "workspace:^",
    "@mami/plugin-hexo": "workspace:^",
    "@mami/plugin-joplin": "workspace:^",
    "@types/node": "^18.8.3",
    "configstore": "^6.0.0",
    "jsonschema": "^1.4.1",
    "koa": "^2.13.4",
    "koa-body": "^6.0.1",
    "koa-logger": "^3.2.1",
    "koa-static": "^5.0.0"
  }
}
